  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dollarshaveclub/eslint-config-es5-only/issues"
  },
  "homepage": "https://github.com/dollarshaveclub/eslint-config-es5-only#readme",
  "peerDependencies": {
    "eslint": ">= 3"
  },
  "devDependencies": {
    "acorn": "^5.1.2",
    "glob": "^7.1.2"
  }
